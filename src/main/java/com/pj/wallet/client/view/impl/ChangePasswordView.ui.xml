<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:vc="urn:import:com.pj.wallet.client.validation.component">

	<g:HTMLPanel>
		<label>Change Password</label>
		<vc:TextBox ui:field="oldPassword" required="true"
			validate="true" />
		<vc:TextBox ui:field="newPassword" required="true"
			validate="true" />
		<vc:TextBox ui:field="confirmNewPassword" required="true"
			validate="true" />
		<g:Button ui:field="submitButton" text="Submit" title="Submit" />
	</g:HTMLPanel>
</ui:UiBinder>