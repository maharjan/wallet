<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.pj.wallet.client.resources.WalletResources"
		field="res" />

	<g:HTMLPanel styleName="{res.walletCss.headerMain}">
		<g:HTMLPanel styleName="{res.walletCss.submenuMain}">
			<g:FlowPanel ui:field="tabPanel" />
			<div style="clear: both;" />
		</g:HTMLPanel>
		<g:FlowPanel ui:field="tabContentContainer" styleName="{res.walletCss.subTabContainer}" />
	</g:HTMLPanel>
</ui:UiBinder> 