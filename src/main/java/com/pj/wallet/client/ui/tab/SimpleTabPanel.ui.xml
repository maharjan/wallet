<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.pj.wallet.client.resources.WalletResources"
		field="res"></ui:with>

	<g:HTMLPanel>
		<g:HTMLPanel>
			<g:FlowPanel ui:field="tabPanel" />
			<div style="clear: both;" />
		</g:HTMLPanel>
		<g:FlowPanel  styleName="{res.walletCss.subTabContainer}">
			<g:FlowPanel ui:field="childContainer"/>
			<g:FlowPanel ui:field="tabContentContainer"/>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 